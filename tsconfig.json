{
  "compilerOptions": {
    "alwaysStrict": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "declaration": true,
    "moduleResolution": "node",
    "charset": "utf8",
    "allowJs": false,
    "target": "es2019",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": false,
    "importHelpers": true,
    "skipDefaultLibCheck": true,
    "allowUnreachableCode": false,
    "pretty": true
  },
  "exclude": ["node_modules", "dist"]
}
